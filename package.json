{
  "name": "obsidian-notechat-plugin",
  "version": "1.0.0",
  "description": "An Obsidian plugin for having interactive AI conversations with your notes to explore, expand, and extract insights",
  "main": "build/main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build:prod": "tsc -noEmit -skipLibCheck --skipDefaultLibCheck && node esbuild.config.mjs production",
    "build:testing": "node build.mjs && cp build/main.js .",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "chat",
    "ai",
    "openai",
    "notes"
  ],
  "author": "NoteChat Team",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@esbuild-plugins/tsconfig-paths": "^0.1.2",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^14.2.1",
    "@types/classnames": "^2.3.4",
    "@types/highlight.js": "^10.1.0",
    "@types/jest": "^29.5.12",
    "@types/markdown-it": "^14.1.2",
    "@types/marked": "^4.3.0",
    "@types/node": "^18.19.42",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/scheduler": "^0.26.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "builtin-modules": "3.3.0",
    "classnames": "^2.5.1",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "markdown-it": "^14.1.0",
    "obsidian": "^1.8.7",
    "ts-jest": "^29.1.2",
    "tslib": "^2.4.0",
    "typescript": "~5.3.3",
    "whatwg-fetch": "^3.6.20"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.0",
    "@langchain/core": "^0.3.40",
    "@langchain/openai": "^0.3.17",
    "axios": "^1.6.7",
    "clsx": "^2.1.1",
    "highlight.js": "^11.11.1",
    "langchain": "^0.3.19",
    "lodash": "^4.17.21",
    "lucide-react": "^0.501.0",
    "marked": "^4.3.0",
    "mini-debounce": "^1.0.8",
    "mobx": "^6.13.7",
    "mobx-react-lite": "^4.1.0",
    "nanoid": "^5.1.3",
    "openai": "^4.98.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-virtualized": "^9.22.5",
    "tailwind-merge": "^3.2.0",
    "zod": "^3.24.2"
  }
}
